# compose.yaml -> litellm service settings
# copy to: .env.litellm
# LiteLLM Proxy auth
LITELLM_MASTER_KEY=sk-litellm-local

# AWS Bedrock credentials for LiteLLM Proxy
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION_NAME=us-east-1

# Azure AI Foundry settings for LiteLLM Proxy
AZURE_FOUNDRY_API_BASE=
AZURE_FOUNDRY_API_KEY=

# Vertex AI settings for LiteLLM Proxy
VERTEX_PROJECT_ID=
VERTEX_LOCATION=us-central1
# Put your service account JSON at:
# ./secrets/vertex-service-account.json

# Optional provider keys for additional routes in litellm_config.yaml
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
