# -------------------------------
# Docker Compose / LiteLLM Proxy
# -------------------------------
# docker compose reads these values for postgres and litellm services.
LITELLM_MASTER_KEY=sk-litellm-master-key
UI_USERNAME=admin
UI_PASSWORD=change-me

POSTGRES_DB=posgtres_db
POSTGRES_USER=postgres_user
POSTGRES_PASSWORD=posgtes_pass
DATABASE_URL=postgresql://postgres_user:posgtes_pass@postgres:5432/posgtres_db

AWS_ACCESS_KEY_ID=AKIAEXAMPLE
AWS_SECRET_ACCESS_KEY=example-secret-access-key
AWS_REGION=ap-northeast-1

# -------------------------------
# chat.py application
# -------------------------------
# chat.py reads these values when you run: uv run chat.py ...
LITELLM_BASE_URL=http://localhost:4000/v1
# Set a Virtual Key generated from LiteLLM UI or /key/generate.
LITELLM_API_KEY=sk-litellm-user-key
SESSION_DB_PATH=sessions.sqlite3
